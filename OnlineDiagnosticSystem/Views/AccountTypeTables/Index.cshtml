@model IEnumerable<DatabaseLayer.AccountTypeTable>

@{
    ViewBag.Title = "Index";
}


    <p>
        @Html.ActionLink("Create New", "Create", "AccountTypeTables", new { @class = "genric-btn info" })


    </p>
<table class="table">
    <tr>
        <th>
            Account Types
        </th>
        <th></th>
    </tr>

@foreach (var item in Model) {
    <tr>
        <td>
            @Html.DisplayFor(modelItem => item.Name)
        </td>
        <td>
            @Html.ActionLink("Edit", "Edit", new { id = item.AccountTypeID }, new { @class = "genric-btn primary" })
            @Html.ActionLink("Delete", "Delete", new { id = item.AccountTypeID }, new { @class = "genric-btn danger" })

        </td>
    </tr>
}

</table>
